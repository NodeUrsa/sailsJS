    <link rel="stylesheet" type="text/css" href="/css/metro-bootstrap.min.css"/>
    <link rel="stylesheet" type="text/css" href="/css/iconFont.min.css"/>
    <link rel="stylesheet" type="text/css" href="/css/styles.css"/>
    <link rel="stylesheet" type="text/css" href="/css/admin.css"/>
    <link rel="stylesheet" type="text/css" href="/css/worker.css"/>
    <link rel="stylesheet" type="text/css" href="/css/fullcalendar.css"/>
    <link rel="stylesheet" type="text/css" href="/css/jquery.timepicker.css"/>
    <link rel="stylesheet" type="text/css" href="/css/nanoscroller.css"/>
    <style type="text/css">
        .field-has-error {
            border-color: #e51400;
        }
        .span3.span3-for-column {
            margin-right: 40px;
        }
    </style>
    <link rel="icon" type="image/x-icon" href="/favicon.ico"/>
</head>
<body class="metro">
    <script type="text/javascript">
        var require = {
            urlArgs: 'bust=' + (new Date()).getTime(),
            paths: {
                jquery              : 'libs/jquery/jquery',
                jquery_ui           : 'libs/jquery-ui/jquery-ui.min',
                pjquery_ui_comonents: 'libs/jquery-ui',
                jquery_cookie       : 'libs/jquery.cookie/jquery.cookie',
                underscore          : 'libs/underscore/underscore-min',
                backbone            : 'libs/backbone/backbone-min',
                backboneExt         : 'libs/backbone/backboneExt',
                marionette          : 'libs/backbone/backbone.marionette',
                syphon              : 'libs/backbone/backbone.syphon',
                validation          : 'libs/backbone/backbone.validation.amd',
                templates           : '../templates',
                router              : 'routers/router',
                metro               : 'libs/metro/metro',
                moment              : 'libs/moment/moment',
                fullcalendar        : 'libs/fullcalendar/fullcalendar',
                nanoscroller        : 'libs/nanoscroller/nanoscroller',
                session             : 'libs/common/session',
                text                : 'libs/require/text',
                timepicker          : 'libs/jquery/jquery.timepicker'
            },

            shim: {
               'fullcalendar': {
                    deps: ['jquery', 'jquery_ui'],
                    exports: 'fullcalendar'
                },
                'nanoscroller': {
                    deps: ['jquery'],
                    exports: 'nanoscroller'
                },
                'backboneExt': {
                    deps: ['backbone'],
                    exports: 'backboneExt'
                },
                'marionette': {
                    deps: ['backboneExt'],
                    exports: 'marionette'
                },
                'jquery_ui': ['jquery'],

                'timepicker': {
                    deps: ['jquery'],
                    exports: 'timepicker'
                },
                'syphon': {
                    deps: ['marionette']
                },
                'validation': {
                    deps: ['marionette', 'jquery']
                }
            }
        };
    </script>
    <%- body %>
</body>
